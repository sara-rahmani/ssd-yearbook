<% for (let i = 0; i < profiles.length; i++) { %> 
  <% if(profiles[i].id === profileId) { %>
  <h2><%= profiles[i].username%></h2>
  <% if(profiles[i].picturePath) { %>
    <img src="<%= profiles[i].picturePath %>" alt="<%= profiles[i].name %>" />
  <% } %>
  <ul>
    <% for (let j = 0; j < profiles[i].interests.length; j++) { %>
      <li><%= profiles[i].interests[j] %></li>
    <% } %>
  </ul>
  <li>
    <% if (roles.includes("Admin") || roles.includes("User") || reqInfo.username == profiles[i].username ) { %>

  <a href="/user/register/<%= profileId %>"><button>Edit</button></a>
  <%}%>
  <% } %> 
<% } %>
